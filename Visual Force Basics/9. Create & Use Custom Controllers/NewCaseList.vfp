<apex:page name="NewCaseList" controller="NewCaselistController" >
        <tr> 
            <th> Case Id </th> <th> Case Number </th>
        </tr>
    <apex:repeat value="{! newCases }" var="case" >
        <tr>
            <td> "{! case.Id }" </td> <td> "{! case.CaseNumber }" </td>
        </tr>
    </apex:repeat>
</apex:page>
